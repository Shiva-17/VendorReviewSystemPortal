
@section head{
    <style type="text/css">
    body
    {
        
        background-image:url('/images/signup2.jpg');
        background-size:cover;
        background-repeat:no-repeat;
        background-position:center;
        margin:auto;
        
        
        
    }
    #customers {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

#customers td, #customers th {
  border: 1px solid #ddd;
  padding: 8px;
}

#customers tr:nth-child(even){background-color: #f2f2f2;}

#customers tr:hover {background-color: #ddd;}

#customers th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #04AA6D;
  color: white;
}
    
    </style>
}

<h1>Complaints Raised By Vendors on Foodies</h1>
<br/>
<div class="container">
    <table class="table table-bordered table-hover" id="customers">
        <tr class="bg-primay text-dark">
            <th>VendorUserID</th>
            <th>VendorName</th>
            <th>VendorLocation</th>
          <th>FoodieUserID</th>
          <th>FoodieName</th>
          <th>RaiseComplaint</th>
        </tr>
        @{
            try
            {
                if(TempData["adminraise"]!=null)
                {
                    foreach(var x in (List<VendorReviewSystemPortal.Models.Complaint>)TempData["adminraise"])
                    {
                        <tr>
                        <td>@x.VendorUserID</td>
                        <td>@x.VendorName</td>
                        <td>@x.VendorLocation</td>
                        <td>@x.FoodieUserID</td>
                        <td>@x.FoodieName</td>
                        <td>@x.RaiseComplaint</td>
                        </tr>
                    }
                }
            }
            catch
            {

            }
        }
    </table>
</div>